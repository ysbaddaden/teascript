object UnlessStatement
  def init(test, body, alternative)
    self.test = test
    self.body = body || new T.Body()
    return self
  end

  def compile
    rs = []
    rs.push('if (!(' + self.test.compile() + ')) {')
    rs.push(self.body.compile())
    rs.push('}')
    return rs.join('')
  end
end

module.exports = UnlessStatement
