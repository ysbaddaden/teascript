TRange = require('../types/range')

def Index(left, index)
  self.left  = left
  self.index = index

  def compile
    if self.index.constructor == TRange
      return 'Array.prototype.slice.call(' +
        self.left.compile() + ', ' +
        self.index.from() + ', ' +
        self.index.to() + ')'
    end
    return self.left.compile() + '[' + self.index.compile() + ']'
  end
end

module.exports = Index
